pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                git 'https://github.com/npoxop/JavaFinalProject'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                sh 'mvn clean compile test -Dtest=CucumberRunner.java'
            }
        }
    }
  post {
          always {
              allure includeProperties: false, jdk: '', results: [[path: 'target/allure-results']]

             }
          }
}